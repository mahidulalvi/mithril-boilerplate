{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:jsx-a11y/recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": ["jsx-a11y", "prettier"],
    "rules": {
      "prettier/prettier": "error"
    }
}
